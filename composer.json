{
    "name": "framework/wecarswoole",
    "description": "wecheche foundational web framework",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.3.0",
        "ext-redis": ">=4.2.1",
        "easyswoole/easyswoole": "3.x",
        "php-di/php-di": "~6.0.0",
        "ramsey/uuid": "~3.8.0",
        "swlib/saber": "~1.0.0",
        "doctrine/annotations": "~1.6.0",
        "monolog/monolog": "~1.24.0",
        "swiftmailer/swiftmailer": "~6.2.0",
        "ocramius/proxy-manager": "~2.2.2",
        "symfony/cache": "~4.2.0",
        "symfony/event-dispatcher": "~4.3.0",
        "psr/event-dispatcher": "^1.0",
        "psr/simple-cache": "^1.0",
        "dev/locker": "^1.0",
        "dev/mysql": "^1.0"
    },
    "repositories": {
        "0": {
            "type": "vcs",
            "url": "https://gitlab4.weicheche.cn/dev/locker.git"
        },
        "1": {
            "type": "vcs",
            "url": "https://gitlab4.weicheche.cn/dev/mysql.git"
        },
        "packagist": {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    },
    "autoload": {
        "psr-4": {
            "WecarSwoole\\": "src/",
            "WecarSwoole\\Test\\": "tests/"
        }
    },
    "bin": ["bin/wecarswoole"],
    "require-dev": {
        "swoole/ide-helper": "dev-master"
    }
}
